<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobeliteevents">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".model.HomeActivity"
            android:theme="@style/AppTheme.FullScreen">

        </activity>
        <activity android:name=".Activitys.ForgetPassActivity"
            android:theme="@style/AppTheme.FullScreen"/>
        <activity android:name=".Activitys.Main2Activity"
            android:theme="@style/AppTheme.FullScreen"/>

        <activity
            android:name=".Activitys.LoginActivity"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name=".Activitys.SplashScreen"
            android:theme="@style/SplashScreentheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.MainActivity"
            android:theme="@style/AppTheme.FullScreen"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>